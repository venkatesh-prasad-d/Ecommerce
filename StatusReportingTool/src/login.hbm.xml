<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD//EN" 
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 <hibernate-mapping package="com.StatusReportingTool.Bean">
 	<class name ="LoginBean" table ="user_login">
 		<id name="uname" type="string" column="emp_id"></id>
 		<property name="name" type="string" column="emp_name"/>
 		<property name="pass" column="password" type="string"></property>
 	</class>
 </hibernate-mapping>