<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD//EN" 
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
 <hibernate-mapping package="com.StatusReportingTool.Bean">
 	
 	<class name="RegisterBean" table="user_table">

 		<id name="uid" type="string" column="emp_id"/>
 		<property name="name" column="emp_name" type="string"/>
 		<property name="pass" column="password" type="string"></property>
 	
 	</class>
 
 </hibernate-mapping>