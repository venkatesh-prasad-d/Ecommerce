<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD//EN" 
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
 <hibernate-mapping package="com.StatusReportingTool.Bean">
 	
 	<class name="ReportDateBean" table="emp_data">
		<id name="id" type="int" column="id"></id>
 		<property name="emp_id" type="string" column="emp_id"/> 		
 		<property name="startDate" column="startDate" type="timestamp"></property>
 		<property name="endDate" column="endDate" type="timestamp"></property>
 		
 	
 	</class>
 
 </hibernate-mapping>